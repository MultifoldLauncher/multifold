{
  "version": 2,
  "projects": {
    "auth-microsoft": {
      "root": "packages/auth-microsoft",
      "sourceRoot": "packages/auth-microsoft/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/auth-microsoft/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/auth-microsoft"
          ],
          "options": {
            "jestConfig": "packages/auth-microsoft/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "auth-mojang": {
      "root": "packages/auth-mojang",
      "sourceRoot": "packages/auth-mojang/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/auth-mojang/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/auth-mojang"
          ],
          "options": {
            "jestConfig": "packages/auth-mojang/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/core"
          ],
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "launcher-app": {
      "root": "apps/launcher/app",
      "sourceRoot": "apps/launcher/app/src",
      "projectType": "application",
      "prefix": "launcher-app",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/launcher/app",
            "main": "apps/launcher/app/src/main.ts",
            "tsConfig": "apps/launcher/app/tsconfig.app.json",
            "assets": [
              "apps/launcher/app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/launcher/app/src/environments/environment.ts",
                  "with": "apps/launcher/app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "launcher-app:build"
          }
        },
        "export": {
          "executor": "nx-electron:package",
          "options": {
            "name": "launcher/app",
            "frontendProject": "launcher-frontend",
            "outputPath": "dist/apps/launcher/app/packages"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/launcher/app/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/launcher/app"
          ],
          "options": {
            "jestConfig": "apps/launcher/app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "launcher-frontend": {
      "root": "apps/launcher/frontend",
      "sourceRoot": "apps/launcher/frontend",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/launcher/frontend",
            "outputPath": "dist/apps/launcher/frontend"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "launcher-frontend:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "launcher-frontend:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "launcher-frontend:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/launcher/frontend"
          ],
          "options": {
            "jestConfig": "apps/launcher/frontend/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/launcher/frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "multifold": {
      "root": "packages/multifold",
      "sourceRoot": "packages/multifold/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/multifold/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/multifold"
          ],
          "options": {
            "jestConfig": "packages/multifold/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "launcher-frontend"
}
