{
  "npmScope": "multifold",
  "affected": {
    "defaultBase": "master"
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "auth-microsoft": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    },
    "auth-mojang": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    },
    "component-java": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    },
    "component-minecraft": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    },
    "core": {
      "tags": []
    },
    "launcher-app": {
      "tags": []
    },
    "launcher-frontend": {
      "tags": []
    },
    "multifold": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    }
  }
}
