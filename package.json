{
  "name": "multifold",
  "version": "0.1.0",
  "description": "The next-generation cross-platform Minecraft launcher",
  "repository": "https://github.com/MultifoldLauncher/multifold",
  "author": "Cubxity <contact@cubxity.dev>",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "nx build",
    "export": "nx build",
    "lint": "nx lint",
    "test": "nx test",
    "build:all": "nx run-many --all --target=build",
    "export:all": "nx run-many --all --target=export",
    "lint:all": "nx run-many --all --target=lint",
    "test:all": "nx run-many --all --target=test",
    "export:launcher": "nx export launcher-frontend && nx build launcher-app --prod && nx run launcher-app:export",
    "serve:launcher": "nx run-many --target=serve --projects=launcher-frontend,launcher-app --parallel",
    "postinstall": "exitzero electron-builder install-app-deps"
  },
  "dependencies": {},
  "devDependencies": {
    "@emotion/babel-plugin": "11.2.0",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/poppins": "^4.5.0",
    "@mui/material": "^5.0.0",
    "@nrwl/cli": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/next": "^12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/react": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/web": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "babel-jest": "27.0.6",
    "core-js": "^3.6.5",
    "electron": "^14.0.1",
    "eslint": "7.22.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "exitzero": "^1.0.1",
    "glob-parent": "^6.0.1",
    "jest": "27.0.3",
    "next": "11.1.1",
    "nx-electron": "^11.1.3",
    "prettier": "^2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "ts-jest": "27.0.3",
    "tslib": "^2.0.0",
    "typescript": "~4.3.5"
  }
}
